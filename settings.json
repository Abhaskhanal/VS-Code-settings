{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "workbench.tree.indent": 2,
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "indentRainbow.includedLanguages": ["html", "javascript"],
  "workbench.iconTheme": "vscode-icons",
  "indentRainbow.excludedLanguages": ["plaintext", "css", "scss"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.fontFamily": "'Cascadia Code'",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "comments": "#2196F3"
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,
  "indentRainbow.colors": [
    "rgba(255,255,64,0.09)",
    "rgba(127,255,127,0.09)",
    "rgba(255,127,255,0.09)",
    "rgba(79,236,236,0.09)"
  ],
  "workbench.colorTheme": "Andromeda Colorizer",
  "editor.fontSize": 15
}
