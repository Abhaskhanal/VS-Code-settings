{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "workbench.tree.indent": 2,
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "indentRainbow.includedLanguages": ["html", "javascript"],
  "workbench.iconTheme": "vscode-icons",
  "indentRainbow.excludedLanguages": ["plaintext", "css", "scss"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.fontFamily": "'Cascadia Code'",
  "editor.smoothScrolling": true,
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "comments": "#2196F3"
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,
  "indentRainbow.colors": [
    "rgba(255,255,64,0.09)",
    "rgba(127,255,127,0.09)",
    "rgba(255,127,255,0.09)",
    "rgba(79,236,236,0.09)"
  ],
  "editor.fontSize": 15,
  "workbench.list.smoothScrolling": true,
  "workbench.colorTheme": "Andromeda Colorizer",
  "workbench.editor.wrapTabs": true,
  "editor.cursorSmoothCaretAnimation": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "black",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "white",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ]
}
